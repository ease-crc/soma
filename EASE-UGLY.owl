</owl:Class>

<owl:Class rdf:about="http://www.ease-crc.org/ont/EASE-ACT.owl#ThinkAloudTopic">
  <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/EASE-ACT.owl#CommunicationTopic"/>
</owl:Class>

