<owl:Class rdf:about="#Delivering">
  <owl:disjointWith rdf:resource="#Closing"/>
  <owl:disjointWith rdf:resource="#Fetching"/>
  <owl:disjointWith rdf:resource="#Lifting"/>
  <owl:disjointWith rdf:resource="#Opening"/>
  <owl:disjointWith rdf:resource="#Pulling"/>
  <owl:disjointWith rdf:resource="#Pushing"/>
  <owl:disjointWith rdf:resource="#Squeezing"/>
  <rdfs:subClassOf rdf:resource="#Actuating"/>
  <rdfs:comment>A task in which an Agent brings an item that the Agent already carries to a specified target.</rdfs:comment>
