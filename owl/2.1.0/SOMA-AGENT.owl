    <!-- http://www.ease-crc.org/ont/SOMA.owl#Singleton -->


    <owl:Class rdf:about="http://www.ease-crc.org/ont/SOMA.owl#Singleton">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Set"/>
