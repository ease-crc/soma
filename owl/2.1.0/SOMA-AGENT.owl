        <rdfs:domain rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Preference"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent"/>
        <rdfs:comment xml:lang="en">Relates a preference quality to the agent the preference belongs to.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-AGENT.owl"/>
