        <rdfs:comment>A &apos;Set&apos; that contains exactly one member.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-AGENT.owl"/>
    </owl:Class>
