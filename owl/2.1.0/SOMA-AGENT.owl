            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#orders"/>
                <owl:allValuesFrom>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#encapsulates"/>
                        <owl:allValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Description"/>
                    </owl:Restriction>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#describes"/>
                <owl:allValuesFrom rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Preference"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-AGENT.owl"/>
        <rdfs:label xml:lang="en">The relation between a &apos;Preference&apos; and the &apos;Order&apos; that the &apos;Preference&apos; defines over Descriptions of Situations.</rdfs:label>
        <rdfs:label>Preference order</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#PreferenceRegion -->


    <owl:Class rdf:about="http://www.ease-crc.org/ont/SOMA.owl#PreferenceRegion">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#SocialObjectAttribute"/>
        <rdfs:subClassOf>
            <owl:Restriction>
